{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "foreground": "#CB4B16",
          "type": "exit",
          "style": "plain"
        },
        {
          "foreground": "#CECE04",
          "type": "root",
          "style": "plain"
        },
        {
          "properties": {
            "display_host": false,
            "prefix": ""
          },
          "foreground": "#ffffff",
          "type": "session",
          "style": "plain"
        },
        {
          "properties": {
            "prefix": "",
            "style": "short"
          },
          "foreground": "#0973C0",
          "type": "path",
          "style": "plain"
        },
        {
          "properties": {
            "prefix": "<#ffffff>git:</>"
          },
          "foreground": "#C1C106",
          "type": "git",
          "style": "plain"
        },
        {
          "properties": {
            "postfix": "",
            "prefix": "\u2b22 "
          },
          "foreground": "#6CA35E",
          "type": "node",
          "style": "plain"
        },
        {
          "properties": {
            "prefix": " <#fefefe>\u23f1 </> ",
            "style": "austin",
            "threshold": 500
          },
          "foreground": "#ffffff",
          "background": "#8800dd",
          "type": "executiontime",
          "style": "plain"
        },
        {
          "properties": {
            "postfix": "",
            "prefix": "\n",
            "text": "<#CB4B16>❯</><#CACA02>❯</><#4E9A06>❯</>"
          },
          "foreground": "#ffffff",
          "type": "text",
          "style": "plain"
        }
      ]
    }
  ],
  "version": 3
}
